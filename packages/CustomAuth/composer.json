{
    "name": "mras/custom-auth",
    "description": "Custom Auth",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Mras\\CustomAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mras\\CustomAuth\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mras\\CustomAuth\\Providers\\CustomAuthServiceProvider"
            ]
        }
    }
}
